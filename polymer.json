{
  "entrypoint": "index.html",
  "shell": "src/corpopong-app/corpopong-app.html",
  "fragments": [
    "src/dashboard/dashboard-fragment.html",
    "src/match/open-match-details.html",
    "src/match/match-create-overlay.html",
    "src/match/match-details.html",
    "src/player/player-details.html",
    "src/player/players-history.html",
    "src/common/notifications-component.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "firebase-messaging-sw.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [{
    "preset": "es5-bundled",
    "js": {
      "minify": false
    },
    "swPrecacheConfig" : "sw-precache-config.js"
  }]
}
